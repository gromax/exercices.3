<table class="table">
  <%if (headers) { %>
    <thead>
    <tr>
      <% _.each(headers, function(header) { %>
        <th scope="col"><%= header %></th>
      <% }); %>
    </tr>
  </thead>
  <% } %>
  <tbody>
    <% _.each(rows, function(row, index) { %>
      <tr>
        <% _.each(row, function(cell) { %>
          <td><%= cell %></td>
        <% }); %>
      </tr>
    <% }); %>
  </tbody>
</table>
