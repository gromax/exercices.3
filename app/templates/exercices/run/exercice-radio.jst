<div class="mb-3">
<% _.each(items, function(paire) { %>
  <div class="form-check">
    <input class="form-check-input" type="radio" name="<%- name %>" id="<%- name %>-<%- paire[0] %>">
    <label class="form-check-label" for="<%- name %>-<%- paire[0] %>">
      <%- paire[1] %>
    </label>
  </div>
<% }); %>
</div>